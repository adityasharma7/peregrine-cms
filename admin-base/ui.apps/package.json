{
  "name": "pcms-admin-base",
  "version": "1.0.0",
  "description": "builds all javascript and vue related components for the peregrine cms admin interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/headwirecom/peregrine-cms.git",
    "directory": "admin-base/ui.apps"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./node_modules/rollup/dist/rollup -c --silent && node ../../buildscripts/buildvue.js admin && node ../../buildscripts/copyfelibs.js && node src/main/buildjs/mdtopage.js",
